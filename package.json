{
  "name": "authrpd",
  "version": "1.0.0",
  "description": "Central Authentication and Master Catalog System",
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production CURRENT_ENV=production node src/server.js",
    "dev": "cross-env NODE_ENV=development CURRENT_ENV=development nodemon src/server.js",
    "start:dev": "cross-env NODE_ENV=development CURRENT_ENV=development node src/server.js",
    "start:prod": "cross-env NODE_ENV=production CURRENT_ENV=production node src/server.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "test": "jest",
    "ensure-partitions": "node scripts/ensure-audit-partitions.js"
  },
  "keywords": [
    "auth",
    "rbac",
    "catalog",
    "jwt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "gelf-pro": "^1.3.4",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.0.1",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "rate-limit-redis": "^5.0.0",
    "sequelize": "^6.35.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
